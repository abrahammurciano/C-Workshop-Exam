{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Build StringTest",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-Wall",
                "-Werror",
                "*.c",
                "-o",
                "${fileDirname}/a.out"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "shell",
            "label": "Doxygen: Generate documentation",
            "command": "doxygen",
            "args": [
                "Doxyfile"
            ],
            "problemMatcher": [
                "$gcc"
            ]
        }
    ],
    "version": "2.0.0"
}